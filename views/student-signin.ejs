  <%- include("header") -%>
        <center>
            <h3>Student Login</h3>
            <form action="/student-view" method="GET">
                <table>
                    <tr>
                        <td>Name:</td>
                        <td>
                            <input type="text" name="student_name" placeholder="enter your name">
                        </td>
                    </tr>
                    <tr>
                        <td>Id:</td>
                        <td>
                            <input type="text" name="student_id" placeholder="enter your id">
                        </td>
                    </tr>
                    <tr>
                        <td>Password:</td>
                        <td>
                            <input type="password" name="student_password" placeholder="enter password">
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <input type="submit" name="submit" value="Login">
                        </td>
                        <td>

                        </td>
                    </tr>
                </table>
            </form>
            <form class="" action="/student-signup" method="GET">
                        <button type="submit" name="button">signup as student</button>
            </form>
        </center>
  <%- include("footer") -%>
