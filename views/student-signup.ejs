<%- include("header") -%>

        <center>
            <h3>Student SignUp</h3>
            <form action="/student-signup" method="POST"   encType="multipart/form-data">
                <table>
                    <tr>
                        <td>Id:</td>
                        <td>
                            <input type="text" name="student_id" placeholder="Enter your id">
                        </td>
                    </tr>
                    <tr>
                        <td>Name:</td>
                        <td>
                            <input type="text" name="student_name" placeholder="Enter full name here">
                        </td>
                    </tr>
                    <tr>
                        <td>Gender:</td>
                        <td>
                            <input type="radio" name="gender" value="male">Male
                            <input type="radio" name="gender" value='female'>Female
                            <input type="radio" name="gender" value='others'>Others
                        </td>
                    </tr>
                    <tr>
                        <td>Password:</td>
                        <td>
                            <input type="password" name="student_pass" placeholder="Enter password here">
                        </td>
                    </tr>
                    <tr>
                        <td>Phone:</td>
                        <td>
                            <input type="phone" name="student_ph" placeholder="98********" maxlength="10">
                        </td>
                    </tr>
                    <tr>
                        <td>Email:</td>
                        <td>
                            <input type="email" name="student_email" placeholder="a***@gmail.com">
                        </td>
                    </tr>
                    <tr>
                      <td>upload photo:</td>
                      <td>  <input type="file" name="sampleFile" /></td>



                      </tr>
                    <tr>
                        <td>
                            <input type="submit" name="submit" value="upload!">


                        </td>

                    </tr>
                </table>
            </form>

            <br>
            <form class="" action="/student-signin" method="GET">
              Already signed in? <button type="submit" name="button">login here</button>

            </form>
        </center>
<%- include("footer") -%>
